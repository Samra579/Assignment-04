def main():

    try:
        curr_value = int(input("Enter a number: "))
    except ValueError:
        print("Please enter a valid integer.")
        return

    while curr_value < 100:
        curr_value *= 2
        print(curr_value, end=' ')

    print()

if __name__ == '__main__':
    main()
     

